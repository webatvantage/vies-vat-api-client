{
	"name": "webatvantage/vies-vat-api-client",
	"type": "tool",
	"description": "EU VAT numbers validation using the VIES Service of the European Commission",
	"keywords": [
		"VIES",
		"VAT",
		"EU",
		"EC"
	],
	"homepage": "https://github.com/webatvantage/vies-vat-api-client",
	"minimum-stability": "stable",
	"license": "MIT",
	"authors": [
		{
			"name": "Michelangelo van Dam",
			"email": "dragonbe@gmail.com",
			"homepage": "https://dragonbe.com",
			"role": "Developer"
		},
		{
			"name": "Tom Six",
			"email": "tom@webatvantage.be",
			"role": "Developer"
		}
	],
	"autoload": {
		"psr-4": {
			"Webatvantage\\Vies\\": "src/Vies"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Webatvantage\\Vies\\Tests\\": "tests/Vies"
		}
	},
	"require": {
		"php": "^7.4 | ^8.0",
		"ext-json": "*",
		"guzzlehttp/guzzle": "^7.9"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.0",
		"webatvantage/php-cs-fixer-config": "^1.0"
	},
	"scripts": {
		"test": "vendor/bin/phpunit",
		"format": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.php"
	},
	"config": {
		"optimize-autoloader": true,
		"sort-packages": true
	}
}
